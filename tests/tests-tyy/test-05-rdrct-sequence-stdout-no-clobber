#IGNORE STDERR
$ dd if=fichier1 of=sortie
$ echo Hello > output > output2 > output3
$ cat output
$ cat output2
$ cat output3
$ rm output output2 output3
$ echo Hello >> output >> output2 >> output3
$ cat output
$ cat output2
$ cat output3
$ rm output output2 output3
$ echo Hello >| output >| output2 >| output3
$ cat output
$ cat output2
$ cat output3
$ rm output output2 output3
$ exit 0
